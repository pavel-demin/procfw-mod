+
+		ReadFile(FIRMWARE_SETTINGS_FILE_D, file_drg, sizeof(file_drg));
+		WriteFile(path_drg, file_drg, sizeof(file_drg));		
+
+		ReadFile(FIRMWARE_SETTINGS_FILE_I, file_irg, sizeof(file_irg));
